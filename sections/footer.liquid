{%- if section.blocks.size > 0 -%}
{%- assign li_w_arr = 'auto,100%,50%,33.3%,25%,20%' | split: ',' -%}
{%- capture section_class -%}
{% if section.settings.has_container %}container px-3 px-sm-0{% endif %} px-3 py-52
{%- endcapture -%}
<footer id="footer" data-section-id="{{ section.id }}" data-section-type="footer" style="background-color:{{section.settings.color_bg}};">
    <div id="footer-top" class=" ">
        <div class="{{section_class}} px-3 py-4">
            <div class="creative-builder">
            {%- for block in section.blocks -%}
                 {% capture block_class %}cb-item{% if block.settings.has_padding %} p-1{% endif %}{% if slider_mode == true %} splide__slide{% else %} col-p{% endif %}{% endcapture %}
                    {% capture block_pcol_css %}--pcol:{{block.settings.width}}%;--pmcol:{{block.settings.width_mobile}}%;{% endcapture %}
                    {% capture block_id %}cb-item-{{block.id}}{% endcapture %}
                    {% capture border_class %}
                    {% if block.settings.border_top %} border-top-md{% endif %}{% if block.settings.border_bottom %} border-bottom-md{% endif %}{% if block.settings.border_left %} border-left-md{% endif %}{% if block.settings.border_right %} border-right-md{% endif %}{% if block.settings.mobile_border_top %} border-top-xs-only{% endif %}{% if block.settings.mobile_border_bottom %} border-bottom-xs-only{% endif %}{% if block.settings.mobile_border_left %} border-left-xs-only{% endif %}{% if block.settings.mobile_border_right %} border-right-xs-only{% endif %}{% endcapture %}

                 {%- case block.type -%}
                    {%- when 'footer-top' -%}
                    <div id="cb-item-{{block.id}}" class="{{block_class}} socials{% if block.settings.right_align %} flex-child-align--right{% endif %} {{block.settings.content_align}}" style="{{block_pcol_css}}">
                        <div class="py-2 py-md-0 d-flex custom-svg-size custom-svg-color" style="--svg-size:{{block.settings.social_size}}px;--svg-color:{{block.settings.color_social}}">
                            {%- assign socials = 'twitter,facebook,linkedin,vimeo,youtube' | split: ',' -%}
                            {%- for social_name in socials -%}
                            {%- if block.settings[social_name] == true -%}
                            {%- assign social_url_setting = 'social_icon_' | append: social_name -%}
                            <a class="d-flex justify-content-center pr-3 pr-lg-4" href="{{settings[social_url_setting] | default: '#'}}">
                                <svg>
                                    <use xlink:href="#global__symbols--social-{{social_name}}" xmlns:xlink="http://www.w3.org/1999/xlink"></use>
                                </svg>
                            </a>
                            {%- endif -%}
                            {%- endfor -%}
                        </div>   
                    </div>   
                    {% if block.settings.payment_logo != blank %}
                    <div id="cb-item-{{block.id}}" class="{{block_class}}{{border_class}} image{% if block.settings.right_align %} flex-child-align--right{% endif %}" style="{{block_pcol_css}}">
                        <div class="payment-link">
                            <a href="{{block.settings.link}}">
                            <img src="{{ block.settings.payment_logo | img_url: 'master' }}" alt="{{block.settings.payment_logo.alt}}">
                            </a>
                        </div> 
                    </div> 
                    {% endif %}   

                 {%- endcase -%}
            {%- endfor -%}
            </div>
        </div>
    </div>
    <div id="footer-wrapper" class="{{section_class}}">
        <div class="cb-body col-p-100{% if section.settings.has_padding %}{% if section.settings.has_container %} p-1{% else %} py-1{% endif %}{% endif %}">
            <div class="cb-body-wrapper creative-builder">
                
                {%- assign group_mode = false -%}
                {%- assign slider_mode = false -%}

                {%- for block in section.blocks -%}
                {% capture block_class %}cb-item{% if block.settings.has_padding %} p-1{% endif %}{% if slider_mode == true %} splide__slide{% else %} col-p{% endif %}{% endcapture %}
                {% capture block_pcol_css %}--pcol:{{block.settings.width}}%;--pmcol:{{block.settings.width_mobile}}%;{% endcapture %}
                {% capture block_id %}cb-item-{{block.id}}{% endcapture %}
                {% capture border_class %}
                {% if block.settings.border_top %} border-top-md{% endif %}{% if block.settings.border_bottom %} border-bottom-md{% endif %}{% if block.settings.border_left %} border-left-md{% endif %}{% if block.settings.border_right %} border-right-md{% endif %}{% if block.settings.mobile_border_top %} border-top-xs-only{% endif %}{% if block.settings.mobile_border_bottom %} border-bottom-xs-only{% endif %}{% if block.settings.mobile_border_left %} border-left-xs-only{% endif %}{% if block.settings.mobile_border_right %} border-right-xs-only{% endif %}{% endcapture %}
            
                {%- case block.type -%}
                {%- when 'banner' -%}
                {%- assign color_bg = block.settings.color_bg -%}
                
                <div id="{{block_id}}" class="position-relative {{block_class}}{{border_class}} text-pos-left custom-min-height" style="{{block_pcol_css}}--min-height:{{block.settings.mobile_height}}px;--min-height-md:{{block.settings.height}}px" {{ block.shopify_attributes }}>
                    {%-if block.settings.cta_text != blank -%}
                    <div class="banner-content-wrapper text-vpos-top{% if block.settings.bg_image != blank %} bgimage-center-no-repeat lazyload{% endif %}" style="background-color:{{color_bg}};"{% if block.settings.bg_image != blank %}{% assign img_url = block.settings.bg_image | img_url: "master" %} data-bg="{{img_url}}"{% endif %}>
                    {%-else-%}
                    <a href="{{block.settings.linked_url}}" class="banner-content-wrapper text-vpos-top{% if block.settings.bg_image != blank %} bgimage-center-no-repeat lazyload{% endif %}" style="background-color:{{color_bg}};"{% if block.settings.bg_image != blank %}{% assign img_url = block.settings.bg_image | img_url: "master" %} data-bg="{{img_url}}"{% endif %}>
                    {%-endif-%}
                        <div class="banner-texting p-2 position-absolute custom-absolute-pos {{block.settings.text_align}}{{' without--fx '}}{%-if block.settings.cta == blank-%}{{' without--cta '}}{%-endif-%}{{' without--linklist '}}" style="--top-side:{{block.settings.side_padding_vert}}%;--left-side:{{block.settings.side_padding_hori}}%;transform: translate({% if block.settings.side_padding_hori > 20 %}-50%{% else %}0{% endif %},{% if block.settings.side_padding_vert > 20 %}-50%{% else %}0{% endif %});">
                            {%-if block.settings.heading != blank-%}
                            <div class="b-heading h3{% if block.settings.heading_shadow == true %} has--text-shadow{% endif %}{% if block.settings.heading_bgcolor_use %} p-2 custom-bgcolor{% else %} mb-3{% endif %} custom-font-size custom-font-weight custom-color" style="--bg-color:{{block.settings.heading_bgcolor}};--font-weight:{{block.settings.heading_weight}};--font-size-lg:{{block.settings.heading_size}}px;--font-size-md:{{block.settings.heading_size | times: 0.7 }}px;--font-size:34px;--color:{{block.settings.heading_color}};">
                            {{block.settings.heading}}
                            </div>
                            {%-endif-%}
                
                            {%-if block.settings.cap != blank-%}
                            <div class="b-caption {%if block.settings.cap_shadow == true %} has--text-shadow{%-endif-%}{% if block.settings.cap_bgcolor_use %} p-2 mb-4 custom-bgcolor{% else %} mb-4{% endif %} custom-font-size custom-color custom-font-weight" style="--bg-color:{{block.settings.cap_bgcolor}};--font-weight:{{block.settings.cap_weight}};--font-size-lg:{{block.settings.cap_size}}px;--font-size-md:{{block.settings.cap_size | times: 0.7 }}px;--font-size:24px;--color:{{block.settings.cap_color}};">
                            {{block.settings.cap}}
                            </div>	
                            {%-endif-%}
                
                            {%-if block.settings.cta_text != blank-%}
                            <a class="{% if block.settings.modern %}btn-modern{% else %}btn-squared custom-bgcolor{% endif %} cta custom-font-size custom-font-weight custom-color" href="{{block.settings.linked_url}}" style="--font-weight:{{block.settings.cta_weight}};--font-size:18px;--font-size-md:{{block.settings.cta_size}}px;--color:{{block.settings.cta_color}};{% if block.settings.modern == false %}--bg-color:{{block.settings.cta_bgcolor}}{% endif %}">{{block.settings.cta_text}}</a>
                            {%-endif-%}
                        </div>
                    {%-if block.settings.cta_text == blank-%}
                    </a>{%-else-%}
                    </div>{%-endif-%}
                </div>
                        
                {%- when 'promotion' -%}
                {%- assign color_bg = block.settings.color_bg -%}
                
                {%- if block.settings.icon_pos == 'column' or block.settings.icon_pos == 'column-reverse' -%}
                {%- assign promo_class = 'position-absolute h-100 w-100 d-flex flex-' | append: block.settings.icon_pos -%}
                {%- assign promo_class = promo_class | append: ' align-items-' | append: block.settings.hori_alignment -%}
                {%- assign promo_class = promo_class | append: ' justify-content-' | append: block.settings.vert_alignment -%}
                {%- else -%}
                {%- assign promo_class = 'position-absolute h-100 w-100 d-flex flex-column flex-md-' | append: block.settings.icon_pos -%}
                {%- assign promo_class = promo_class | append: ' align-items-center align-items-md-' | append: block.settings.vert_alignment -%}
                {%- assign promo_class = promo_class | append: ' justify-content-center justify-content-md-' | append: block.settings.hori_alignment -%}
                {%- endif -%}

                {%- capture var_style -%}
                --font-size:{{block.settings.font_size}}px;
                --svg-size:{{block.settings.icon_size}}px;
                --font-weight:{{block.settings.font_weight}};
                {%-if block.settings.color_text != empty and block.settings.color_text != 'rgba(0,0,0,0)' -%}--color:{{block.settings.color_text}};--svg-color:{{block.settings.color_text}};{%- endif -%}
                --bg-color:{{color_bg}};
                --min-height:{{block.settings.mobile_height}}px;--min-height-md:{{block.settings.height}}px
                {%- endcapture -%}
                <div id="cb-item-{{block.id}}" class="{{block_class}}{{border_class}} custom-min-height" style="{{block_pcol_css}}{{var_style}}" {{ block.shopify_attributes }}>  
                    {% if block.settings.linked_url != empty %}
                    <a class="w-100 custom-bgcolor position-relative" href="{{block.settings.linked_url}}">
                    {% else %}
                    <div class="w-100 custom-bgcolor position-relative">
                    {% endif %}    
                        <div class="{{promo_class}} custom-absolute-pos" style="--top-side:{{block.settings.side_padding_vert}}%;--left-side:{{block.settings.side_padding_hori}}%">
                            <div class="p-1 lh--1 custom-svg-size custom-svg-color"><svg><use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#global__symbols--{{block.settings.icon}}"></use></svg></div>
                            <div class="p-1 d-flex align-items-center" ><p class="m-0 custom-font-size custom-font-weight custom-color text-center">{{block.settings.title}}</p></div>
                        </div>
                    {% if block.settings.linked_url == empty %}
                    </div>
                    {% else %}
                    </a>
                    {% endif %}
                </div>

                {%- when 'image' -%}
                <div id="cb-item-{{block.id}}" class="{{block_class}}{{border_class}} image{% if block.settings.right_align %} flex-child-align--right{% endif %}" style="{{block_pcol_css}}">
                    {%- assign _class = 'w-100 d-flex flex-' | append: block.settings.image_pos | append: ' ' | append: block.settings.text_align -%}
                    {%- if block.settings.fadeoff -%}
                    {%- assign _class = _class | append: ' p-4 align-items-center of-hidden position-relative' -%}
                    {%- endif -%}
                    {%-if block.settings.linked_url != blank-%}
                    <a href="{{block.settings.linked_url}}" class="{{_class}}" {%if block.settings.text == blank%} style="line-height:0;" {%endif%}>
                    {%-else-%}
                    <div class="{{_class}}">
                        {%-endif-%}
                        {%-if block.settings.image_pos != blank-%}
                        {%-if block.settings.image == blank-%}
                        <div class="cb-item--image">
                        <img class="lazyload w-100" src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==" data-src="https://via.placeholder.com/640x360/ECB39F/ffffff?text=Put%20your%20Image%20here."/>
                        </div>
                        {%-else-%}
                        {%- assign img_url = block.settings.image | img_url: 'master' -%}
                        <div class="{% if block.settings.fadeoff %} fade-off-bg{% endif %}">
                        <img class="lazyload w-100" src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==" data-src="{{img_url}}" alt="{{block.settings.text | strip_html}}"/>
                        </div>
                        {%-endif-%}
                        {%-endif-%}
                        {%- if block.settings.text != empty -%}
                        <div class="cb-item--head{% if block.settings.image_pos == 'row' or block.settings.image_pos == 'row-reverse' %} px-2{% endif %}{% if block.settings.fadeoff %} absolute-bottom pb-2{% endif %}" style="font-weight:{{block.settings.text_weight}};font-size:{{block.settings.text_size}}px;color:{{block.settings.text_color}};">
                        {{block.settings.text}}
                        </div>
                        {%-endif-%}
                        {%-if block.settings.linked_url == blank-%}
                    </div>
                    {%-else-%}
                    </a>
                    {%-endif-%}
                    </div>

                {%- when 'linklist' -%}
                <div id="cb-item-{{block.id}}" class="{{block_class}} linklist" style="{{block_pcol_css}}">
                    {%- if block.settings.links != blank and linklists[block.settings.links] != empty -%}
                    {%- assign _items = linklists[block.settings.links].links -%}
                    <div class="d-flex flex-column w-100 {% if block.settings.content_center %} align-items-center{% endif %}">
                        <div class="pb-3 custom-font-size custom-color custom-font-weight" style="--color:{{block.settings.color_title}};--font-size:{{block.settings.fontsize_title}}px;--font-weight:{{block.settings.fontweight_title}}">{%- if block.settings.title != blank -%}{{block.settings.title}}{%- else -%}{{linklists[block.settings.links].title}}{%- endif -%}</div>
                        <div>
                            <ul class="pl-0{% if block.settings.content_center %} d-flex flex-wrap{% else %} pr-lg-5{% endif %}" style="column-count:{{block.settings.col}};--color:{{block.settings.color_item}};--font-size:{{block.settings.fontsize_item}}px;--font-weight:{{block.settings.fontweight_item}}">
                                {%- for _item in _items -%}
                                <li class="{% if block.settings.content_center %} px-2{% else %} d-table pr-2{% endif %} pb-1 custom-font-size custom-color custom-font-weight"><a href="{{_item.url}}">{{_item.title}}</a></li>
                                {%- endfor -%}
                            </ul>
                        </div>
                    </div>
                    {% else %}
                    <div class="d-flex flex-column w-100 {% if block.settings.content_center %} align-items-center{% endif %}">
                        <div class="pb-3 custom-font-size custom-color custom-font-weight" style="--color:{{block.settings.color_title}};--font-size:{{block.settings.fontsize_title}}px;--font-weight:{{block.settings.fontweight_title}}">MENU TITLE</div>
                        <div>
                            <ul class="pl-0{% if block.settings.content_center %} d-flex flex-wrap{% else %} pr-lg-5{% endif %}" style="column-count:{{block.settings.col}};--color:{{block.settings.color_item}};--font-size:{{block.settings.fontsize_item}}px;--font-weight:{{block.settings.fontweight_item}}">    
                                {%- for i in (1..6) -%}
                                <li class="{% if block.settings.content_center %} px-2{% else %} d-table pr-2{% endif %} pb-1 custom-font-size custom-color custom-font-weight"><a href="#">MENU ITEM</a></li>
                                {%- endfor -%}
                            </ul>
                        </div>
                    </div>
                    {%- endif -%}
                </div>

                {%- when 'newsletter' -%}
                <div id="cb-item-{{block.id}}" class="{{block_class}} newsletter-form justify-content-{{block.settings.content_align}}" style="{{block_pcol_css}}">
                    <form id="cb-item--form-{{section.id}}-{{block.id}}" class="cb-item--form d-flex flex-column" action="{{settings.popup_mailchimp_action}}" method="post" name="mc-embedded-subscribe-form" target="_blank">
                        <div class="d-flex flex-column flex-lg-row align-items-{{block.settings.content_align}} align-items-md-start">
                            <span class="h3 custom-color pr-4 pb-2 pt-1" style="flex:0 0 auto;--color:{{block.settings.color_title}}">{{block.settings.title}}</span>
                            <div class="d-flex flex-column" style="flex:1 1 auto;--border-color:transparent">
                                <div class="d-flex flex-row">
                                    <input id="cb-item--form-email-{{section.id}}-{{block.id}}" class="email mr-2 custom-bgcolor custom-border-color custom-color" type="email" name="EMAIL" placeholder="{{'general.mailchimp_popup.enter_your_email' | t}}" required="required" style="flex:1 1 auto;--bg-color:{{block.settings.color_input}};--color:{{block.settings.color_input_text}};"/>
                                    <button class="btn subscribe custom-bgcolor custom-border-color custom-color" type="submit" {% if settings.newsletter_terms and settings.newsletter_terms_page != blank %}disabled{% endif %} style="flex:0 0 auto;--bg-color:{{block.settings.color_button}};--color:{{block.settings.color_button_text}};">{{'general.mailchimp_popup.join_us' | t}}</button>
                                </div>
                                <label for="cb-item--form-email-{{section.id}}-{{block.id}}" class="mt-2 mb-1"></label>
                                {% if settings.newsletter_terms and settings.newsletter_terms_page != blank %}
                                {% assign newsletter_terms_page = '/pages/' | append: settings.newsletter_terms_page %}
                                <div class="newsletter-terms-conditions">
                                <label class="d-flex align-items-center mt-0 mb-2">
                                    <input type="checkbox" class="newsletter-terms mr-1 custom-bgcolor custom-border-color" style="--bg-color:{{block.settings.color_input}}"/>
                                    <span class="custom-color" style="--color:{{block.settings.color_terms}};">{{ 'cart.general.terms_conditions_html' | t : terms_conditions_url: "/policies/terms-of-service" ,  privacy_policy_url: "/policies/privacy-policy" }}</span>
                                </label>
                                </div>
                                {% endif %} 
                            </div>
                        </div>
                    </form>
                </div>

                {%- when 'spacing' -%}
                <div id="cb-item-{{block.id}}" class="{{block_class}} spacing justify-content-{{block.settings.line_align}} align-items-center custom-min-height" style="{{block_pcol_css}}--min-height:{{block.settings.mobile_height}}px;--min-height-md:{{block.settings.height}}px;background-color:{{block.settings.color_bg}};">
                    {%- if block.settings.line_thick > 0 -%}
                    <span class="col-p" style="height:{{block.settings.line_thick}}px;background-color:{{block.settings.color_line}};--pcol:{{block.settings.line_width}}%"></span>
                    {%- endif -%}
                </div>

                {%- when 'social' -%}
                <div id="cb-item-{{block.id}}" class="{{block_class}} socials{% if block.settings.right_align %} flex-child-align--right{% endif %} {{block.settings.content_align}}" style="{{block_pcol_css}}">
                    <div class="py-2 py-md-0 d-flex custom-svg-size custom-svg-color" style="--svg-size:{{block.settings.social_size}}px;--svg-color:{{block.settings.color_social}}">
                        {%- assign socials = 'twitter,facebook,linkedin,vimeo,youtube' | split: ',' -%}
                        {%- for social_name in socials -%}
                        {%- if block.settings[social_name] == true -%}
                        {%- assign social_url_setting = 'social_icon_' | append: social_name -%}
                        <a class="d-flex justify-content-center pr-3 pr-lg-4" href="{{settings[social_url_setting] | default: '#'}}">
                            <svg>
                                <use xlink:href="#global__symbols--social-{{social_name}}" xmlns:xlink="http://www.w3.org/1999/xlink"></use>
                            </svg>
                        </a>
                        {%- endif -%}
                        {%- endfor -%}
                    </div>
                </div>

                {%- when 'text' -%}
                <div id="cb-item-{{block.id}}" class="{{block_class}} htmltext {{block.settings.content_align}}" style="{{block_pcol_css}}">
                    <div class="py-2 py-md-0 no-margin--p d-flex custom-color" style="--color:{{block.settings.color_text}}">
                        <p>{{block.settings.text}}</p>
                    </div>
                </div>

                {%- when 'group-start' -%}
                <div id="cb-item-{{block.id}}" class="{{block_class}} flex-wrap{% if section.settings.has_container %} has-container{% endif %}{% if block.settings.hori_scroll_mobile == true %} rt-mobile-snapscroll{% endif %}" style="{{block_pcol_css}}">
                    {%- assign group_mode = true -%}
                    
                {%- when 'group-end' -%}
                {%- if group_mode == true -%}
                {%- assign group_mode = false -%}
                </div>
                {%- endif -%}

                {%- endcase -%}
                {%- endfor -%}
            </div>
        </div>
    </div>

    <div id="footer-bottom" >
        <div class="{{section_class}} px-3 py-4">
            <div class="creative-builder">
            {%- for block in section.blocks -%}
                  
                 {%- case block.type -%}
                    {%- when 'footer-bottom' -%}
                    <div id="cb-item-{{block.id}}" class="{{block_class}} braningl{% if block.settings.right_align %} flex-child-align--right{% endif %} {{block.settings.content_align}}" style="">
                        {% if block.settings.image != blank %}
                        <div class="branding-logo">
                            <img src="{{ block.settings.image | img_url: 'master' }}" alt="{{block.settings.payment_logo.alt}}">
                        </div>
                        {% endif %}   
                        <div class="copy-right">
                            {{block.settings.copyright}}
                        </div>
                    </div>   
          

                 {%- endcase -%}
            {%- endfor -%}
            </div>
        </div>
    </div>
</footer>
{%- endif -%}
{% schema %}

{
    "name": "Footer",
    "class": "index-section section-wrapper",
    "settings": [
        {
            "type": "header",
            "content": "Layout"
        },
        {
            "type": "checkbox",
            "id": "has_container",
            "label": "Within Container",
            "default": true
        },
        {
            "type": "checkbox",
            "id": "has_padding",
            "label": "Add padding",
            "default": false
        },
        {
            "type": "color",
            "id": "color_bg",
            "label": "Background color",
            "default": "#1a1a1a"
        }
    ],
    "blocks": [
        {
            "type": "group-start",
            "name": "▛ Group Start",
            "settings": [
                {
                    "type": "header",
                    "content": "General"
                },
                {
                    "type": "range",
                    "id": "width",
                    "min": 0,
                    "max": 100,
                    "step": 1,
                    "unit": "%",
                    "label": "Width",
                    "default": 100
                },
                {
                    "type": "range",
                    "id": "width_mobile",
                    "min": 0,
                    "max": 100,
                    "step": 1,
                    "unit": "%",
                    "label": "Mobile width",
                    "default": 100
                },
                {
                    "type": "color",
                    "id": "color_bg",
                    "label": "Background color",
                    "default": "#fff"
                },
                {
                    "type": "checkbox",
                    "id": "hori_scroll_mobile",
                    "label": "Horizontal scroll on mobile",
                    "default": false
                }
            ]
        },
        {
            "type": "group-end",
            "name": "▙ Group End"
        },
        {
            "type": "banner",
            "name": "Banner",
            "settings": [
                {
                    "type": "header",
                    "content": "General"
                },
                {
                    "type": "range",
                    "id": "width",
                    "min": 0,
                    "max": 100,
                    "step": 1,
                    "unit": "%",
                    "label": "Width",
                    "default": 50
                },
                {
                    "type": "range",
                    "id": "width_mobile",
                    "min": 0,
                    "max": 100,
                    "step": 1,
                    "unit": "%",
                    "label": "Mobile width",
                    "default": 50
                },
                {
                    "type": "range",
                    "id": "height",
                    "min": 20,
                    "max": 1000,
                    "step": 10,
                    "unit": "px",
                    "label": "Height",
                    "default": 250
                },
                {
                    "type": "range",
                    "id": "mobile_height",
                    "min": 10,
                    "max": 500,
